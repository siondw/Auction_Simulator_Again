{
    "name": "Python: Flask",
    "type": "python",
    "request": "launch",
    "program": "${workspaceFolder}/app.py",
    "args": [
        "run",
        "--no-debugger",
        "--no-reload"
    ],
    "env": {
        "FLASK_APP": "app.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "0"
    },
    "jinja": true
}
